        <script>
            function aramayap(nesne) {
                if (document.getElementById(nesne).value != "") {
                    $.ajax({
                        type: 'post',
                        data: '{deger:"' + document.getElementById(nesne).value + '"}',
                        url: '/Default.aspx/Aranilacak',
                        dataType: 'json',
                        contentType: 'application/json; charset=utf-8',
                        success: function (dnn) {
                            if (dnn.d != "")
                                document.location.href = "/news-search/1/" + dnn.d;
                        }
                    })
                }
            }

            $(document).ready(function () {
                $("#aranacak").keyup(function (event) {
                    if (event.keyCode == 13) {
                        if ($(this).val() != "")
                            aramayap('aranacak')
                    }
                });

                $("#aranacak2").keyup(function (event) {
                    if (event.keyCode == 13) {
                        if ($(this).val() != "")
                            aramayap('aranacak2')
                    }
                });
            })


        </script>

