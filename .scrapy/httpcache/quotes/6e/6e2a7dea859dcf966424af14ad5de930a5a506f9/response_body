<script>
    jQuery("body").on("click", ".news-link", function (event) {
        event.stopPropagation();
        var post_id = $(this).data("post-id");
        var link = $(this).attr("href");
        var settings = {
            url: 'http://brexit-unplugged.co.uk/wp-admin/admin-ajax.php',
            data: params = 'action=update_views_ajax&token=9856d5638c&wpp_id=' + post_id,
            method: "POST"
        }
        $.ajax(settings).done(function (result) {
            console.log(result);
            var device_width = window.outerWidth;
            if(device_width > 767){
            window.open(link, "_blank");
            }else{
                window.location.href = link;
            }
        })

        return false;
    });

</script>
